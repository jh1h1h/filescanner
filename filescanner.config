[Credentials - Assignment Patterns]
Linux Command: grep -r -E 'KEYWORDS' "$SEARCH_ROOT" EXTENSIONS
Linux Example: grep -r -E 'password\s*=|password:|PASSWORD=' "$SEARCH_ROOT" --include="*.env" --include="*.yml" --include="*.yaml"
Windows Command: Select-String -Path "$SEARCH_ROOT" -Pattern 'KEYWORDS' -Recurse EXTENSIONS
Windows Example: Select-String -Path "$SEARCH_ROOT" -Pattern 'password\s*=|password:|PASSWORD=' -Recurse -Include *.env,*.yml,*.yaml
# Searches for actual credential assignments (not just the word "password")
# Focuses on configuration syntax where credentials are actually set
# Format: key=value, key: value, KEY=value
Keywords: password\s*=, password:, PASSWORD=, passwd\s*=, pwd\s*=, secret\s*=, SECRET=, api_key\s*=, apikey\s*=, API_KEY=, access_key\s*=, private_key\s*=, db_password\s*=, database_password\s*=, mysql_password\s*=, postgres_password\s*=
Extensions: *.env, *.env.local, *.env.production, *.env.staging, *.yml, *.yaml, *.properties, *.toml, *.ini, *.cnf

[API Keys - Provider Specific]
Linux Command: grep -r -E 'KEYWORDS' "$SEARCH_ROOT" EXTENSIONS
Linux Example: grep -r -E 'AKIA[A-Z0-9]{16}|ghp_[a-zA-Z0-9]{36}|sk_live_[a-zA-Z0-9]{24}' "$SEARCH_ROOT" --include="*.env" --include="*.py" --include="*.js"
Windows Command: Select-String -Path "$SEARCH_ROOT" -Pattern 'KEYWORDS' -Recurse EXTENSIONS
Windows Example: Select-String -Path "$SEARCH_ROOT" -Pattern 'AKIA[A-Z0-9]{16}|ghp_[a-zA-Z0-9]{36}|sk_live_[a-zA-Z0-9]{24}' -Recurse -Include *.env,*.py,*.js
# Searches for actual API key formats from major providers
# AWS: AKIA prefix + 16 chars, GitHub: ghp_ prefix, Stripe: sk_live_ prefix
# These patterns have very low false positive rates
Keywords: AKIA[A-Z0-9]{16}, ASIA[A-Z0-9]{16}, ghp_[a-zA-Z0-9]{36}, gho_[a-zA-Z0-9]{36}, github_pat_[a-zA-Z0-9]{82}, glpat-[a-zA-Z0-9]{20}, sk_live_[a-zA-Z0-9]{24}, pk_live_[a-zA-Z0-9]{24}, rk_live_[a-zA-Z0-9]{24}, sq0atp-[a-zA-Z0-9]{22}, sq0csp-[a-zA-Z0-9]{43}, sk-[a-zA-Z0-9]{48}, AIza[0-9A-Za-z\-_]{35}, ya29\.[0-9A-Za-z\-_]+, client_secret_[a-zA-Z0-9\-_]{24}
Extensions: *.env, *.env.local, *.env.production, *.py, *.js, *.ts, *.jsx, *.tsx, *.java, *.php, *.rb, *.go, *.cs, *.json, *.yml, *.yaml, *.xml

[Private Keys - Actual Key Material]
Linux Command: grep -r -E 'KEYWORDS' "$SEARCH_ROOT" EXTENSIONS
Linux Example: grep -r -E '-----BEGIN (RSA |DSA |EC |OPENSSH )?PRIVATE KEY-----|-----BEGIN ENCRYPTED PRIVATE KEY-----' "$SEARCH_ROOT" --include="*.pem" --include="*.key"
Windows Command: Select-String -Path "$SEARCH_ROOT" -Pattern 'KEYWORDS' -Recurse EXTENSIONS
Windows Example: Select-String -Path "$SEARCH_ROOT" -Pattern '-----BEGIN (RSA |DSA |EC |OPENSSH )?PRIVATE KEY-----|-----BEGIN ENCRYPTED PRIVATE KEY-----' -Recurse -Include *.pem,*.key
# Searches for actual PEM-formatted private keys (not just files named .key)
# This finds the actual key material header
Keywords: -----BEGIN (RSA |DSA |EC |OPENSSH |ENCRYPTED )?PRIVATE KEY-----, -----BEGIN PGP PRIVATE KEY BLOCK-----, ssh-rsa AAAA[A-Za-z0-9+/]{300}, ssh-ed25519 AAAA[A-Za-z0-9+/]{68}, ecdsa-sha2-nistp256 AAAA
Extensions: *.pem, *.key, *.ppk, *.asc, *.gpg, id_rsa, id_dsa, id_ecdsa, id_ed25519

[High-Value Database Files]
Linux Command: find "$SEARCH_ROOT" -type f FILES
Linux Example: find "$SEARCH_ROOT" -type f \( -name "*.kdbx" -o -name "production.db" -o -name "users.sqlite" \)
Windows Command: Get-ChildItem -Path "$SEARCH_ROOT" -Recurse -File FILES
Windows Example: Get-ChildItem -Path "$SEARCH_ROOT" -Recurse -File -Include *.kdbx,production.db,users.sqlite
# Only searches for high-value, specific database files
# Excludes generic .db files (too many false positives)
# Focuses on: password managers, production DBs, user data
Files: *.kdbx, *.kdb, production.db, production.sqlite, production.sqlite3, prod.db, users.db, users.sqlite, users.sqlite3, passwords.db, passwords.sqlite, accounts.db, accounts.sqlite, credentials.db, credentials.sqlite, *.mdb, *.accdb, *.mdf, *.ldf, *.frm, *.ibd, database.sqlite, database.sqlite3, db.sqlite3, site.db

